        "/user/{uid}": {
            "put": {
                "tags": [
                    "user"
                ],
                "description": "更新用户信息",
                "operationId": "UserController.更新用户信息",
                "parameters": [
                    {
                        "in": "path",
                        "name": "uid",
                        "description": "用户ID,默认为自己的",
                        "type": "integer",
                        "format": "int64"
                    },
                    {
                        "in": "body",
                        "name": "body",
                        "description": "更新用户信息",
                        "schema": {
                            "$ref": "#/definitions/controllers.UserUpdateRequest"
                        }
                    }
                ],
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
        }
