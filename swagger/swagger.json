    "basePath": "/api",
    "paths": {
        "/auth/login": {
            "post": {
                "tags": [
                    "auth"
                ],
                "description": "用户登录",
                "operationId": "AuthController.用户登录",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "用户登录",
                        "schema": {
                            "$ref": "#/definitions/controllers.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
        },
        "/auth/logout": {
            "post": {
                "tags": [
                    "auth"
                ],
                "description": "用户登录",
                "operationId": "AuthController.用户登录",
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
        },
        "/auth/password": {
            "put": {
                "tags": [
                    "auth"
                ],
                "description": "修改密码",
                "operationId": "AuthController.修改密码",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "修改密码",
                        "schema": {
                            "$ref": "#/definitions/controllers.ChangePasswordRequest"
                        }
                    }
                ],
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
        },
        "/auth/register": {
            "post": {
                "tags": [
                    "auth"
                ],
                "description": "客服注册",
                "operationId": "AuthController.客服注册",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "客服注册",
                        "schema": {
                            "$ref": "#/definitions/controllers.RegisterRequest"
                        }
                    }
                ],
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
        },
        "/message/": {
            "post": {
                "tags": [
                    "message"
                ],
                "description": "发送消息",
                "operationId": "MessageController.发送消息",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "消息主体",
                        "schema": {
                            "$ref": "#/definitions/controllers.SendMessageRequest"
                        }
                    }
                ],
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
        },
        "/message/polling": {
            "get": {
                "tags": [
                    "message"
                ],
                "description": "使用ajax轮询接收消息",
                "operationId": "MessageController.接收消息",
                "parameters": [
                    {
                        "in": "query",
                        "name": "domain",
                        "description": "域",
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "email",
                        "description": "邮箱",
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "name",
                        "description": "姓名",
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "random",
                        "description": "固定随机字符串，用以标识当前会话的唯一性",
                        "type": "string"
                    }
                ],
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
        },
        "/message/record": {
            "get": {
                "tags": [
                    "message"
                ],
                "description": "消息记录",
                "operationId": "MessageController.消息记录",
                "parameters": [
                    {
                        "in": "query",
                        "name": "id",
                        "description": "目标ID，默认获取自身",
                        "type": "string"
                    }
                ],
                "responses": {
                    "code.OK": {
                        "description": ""
                    }
                }
            }
